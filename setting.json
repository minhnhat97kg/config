{
  "version": "1.0.1",
  "message": "Bạn nhận được {{amount}} đồng",
  "apps": [
    {
      "packageName": "",
      "regexes": [
        "\\+ VND (?<amount>\\d+([.,]\\d+)?)",
        "\\+(?<amount>\\d+([.,]\\d+)?)VND"
      ],
      "replaces": [
        {
          "from": ",",
          "to": ""
        },
        {
          "from": ".",
          "to": ""
        }
      ]
    }
  ]
}
